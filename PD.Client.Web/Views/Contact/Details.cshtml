@model ContactQueryWithInfoViewModel

<div class="form-group">
    <label for="txtName">Name</label>
    <input id="txtName" asp-for="Name" class="form-control disabled" />
</div>


<div class="pt-2"></div>

<table class="table table-bordered">
    <thead>
        <tr>
            <th scope="col">Contact Info Type</th>
            <th scope="col">Content</th>
            <th width="10%" scope="col"></th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.ContactInfoList.Count; i++)
        {
            <tr>
                <td>@Model.ContactInfoList[i].ContactInfoType</td>
                <td>@Model.ContactInfoList[i].ContactInfoContent</td>
                <td><a href="#" class="btn btn-danger">Delete</a></td>
            </tr>
        }

    </tbody>
</table>